<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="">
  <meta name="author" content="">
  <!-- Favicons -->
  <link href="/images/favicon.png" rel="icon">
  

  <title>Kiwi Exchange Dashboard</title>

  <!-- Custom fonts for this template-->
  <link href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.12.0-2/css/all.min.css" rel="stylesheet"
    type="text/css">
  <link
    href="https://fonts.googleapis.com/css?family=Nunito:200,200i,300,300i,400,400i,600,600i,700,700i,800,800i,900,900i"
    rel="stylesheet">

  <!-- Custom styles for this template-->
  <link href="https://cdn.jsdelivr.net/npm/startbootstrap-sb-admin-2@4.0.7/css/sb-admin-2.min.css" rel="stylesheet">

  <!-- Custom fonnt awesome-->
  <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet">
  <link href="https://cdn.datatables.net/1.10.21/css/dataTables.bootstrap4.min.css" rel="stylesheet">

  <!-- Main CSS File -->
  <link href="https://cdn.datatables.net/1.10.21/css/dataTables.bootstrap4.min.css" rel="stylesheet">
  <link href="/stylesheets/styleDashboard.css" rel="stylesheet">

</head>

<body id="page-top" class="sidebar-toggled">
  <div id="wrapper">

    <!-- Sidebar -->
    <ul class="navbar-nav bg-gradient-primary sidebar sidebar-dark accordion toggled" id="accordionSidebar">
      <!-- Sidebar - Brand -->
      <a class="sidebar-brand d-flex align-items-center justify-content-center" href="/">
        <div class="sidebar-brand-text mx-3 d-block d-sm-block d-md-block"><img src="/images/favicon.png" whidth="60px"
            height="35px" alt="" class="">
          <div class="sidebar-brand-text mx-3">Kiwi Exchange</div>
        </div>
      </a>

      <!-- Divider -->
      <hr class="sidebar-divider my-0">
      <!-- Nav Item - Dashboard -->
      <li id="mnuHome" class="nav-item">
        <a class="nav-link" href="/app">
          <i class="fas fa-fw fa-tachometer-alt"></i>
          <span>Dashboard</span></a>
      </li>
      <!-- Divider -->
      <hr class="sidebar-divider">
      <!-- Heading -->
      <div class="sidebar-heading">
        Application
      </div>
      <!-- Nav Item - Trade -->
      <li id="mnuTrade" class="nav-item">
        <a class="nav-link" href="/app/trade">
          <i class="fas fa-fw fa-line-chart"></i>
          <span>Trade</span></a>
      </li>
      <!-- Nav Item - Wallet -->
      <li id="mnuWallet" class="nav-item">
        <a class="nav-link" href="/app/wallet">
          <i class="fas fa-fw fa-university"></i>
          <span>Wallet</span></a>
      </li>
      <!-- Divider -->
      <hr class="sidebar-divider">
      <!-- Heading -->
      <div class="sidebar-heading">
        Reports
      </div>
      <!-- Nav Item - Market -->
      <li id="mnuMarket" class="nav-item">
        <a class="nav-link" href="/app/market">
          <i class="fas fa-fw fa fa-btc"></i>
          <span>Market</span></a>
      </li>
      <!-- Nav Item - Transactions -->
      <li id="mnuTrans" class="nav-item">
        <a class="nav-link" href="/app/transactions">
          <i class="fas fa-fw fa fa-exchange"></i>
          <span>Transaction</span></a>
      </li>
      <!-- Divider -->
      <hr class="sidebar-divider">
      <!-- Heading -->
      <div class="sidebar-heading">
        Utilities
      </div>
      <!-- Nav Item - Profile -->
      <li id="mnuUser" class="nav-item">
        <a class="nav-link" href="/app/user">
          <i class="fas fa-fw fa-user"></i>
          <span>Profile</span></a>
      </li>
      <!-- Nav Item - Support -->
      <li id="mnuSupport" class="nav-item">
        <a class="nav-link" href="/app/support">
          <i class="fas fa-fw fa-life-ring"></i>
          <span class="">Support</span></a>
      </li>
      <!-- Nav Item - Support -->
      <li id="mnuLogs" class="nav-item">
        <a class="nav-link" href="/app/logs">
          <i class="fas fa-fw fa-list "></i>
          <span class="">Activity Log</span></a>
      </li>
      <!-- Divider -->
      <hr class="sidebar-divider">

      <!-- Sidebar Toggler (Sidebar) -->
      <div class="text-center d-none d-md-inline">
        <button class="rounded-circle border-0" id="sidebarToggle"></button>
      </div>
      <!-- Divider -->
      <hr class="sidebar-divider">
      <!-- Sidebar - Brand -->
      <a class="sidebar-brand d-flex align-items-center justify-content-center" href="/">
        <div class="sidebar-brand-text mx-3 d-block d-sm-block d-md-block"><img src="/images/favicon.png" whidth="60px"
            height="35px" alt="">
          <div class="sidebar-brand-text mx-3">Kiwi Exchange</div>
        </div>
      </a>

    </ul>
    <!-- End of Sidebar -->

    <!-- Content Wrapper -->
    <div id="content-wrapper" class="d-flex flex-column">
      <!-- Main Content -->
      <div id="content">
        <!-- Topbar -->
        <nav class="navbar navbar-expand navbar-light bg-white topbar mb-4 static-top shadow">
          <!-- Sidebar Toggle (Topbar) -->
          <button id="sidebarToggleTop" class="btn btn-link d-md-none rounded-circle mr-3">
            <i class="fa fa-bars"></i>
          </button>

          <div class="sidebar-brand-text mx-3"><img src="/images/favicon.png" whidth="60px" height="35px" alt=""
              class="">
          </div>

          <!-- Topbar Navbar -->
          <ul class="navbar-nav ml-auto">
            <!-- Dropdown - Messages -->
            <div class="dropdown-menu dropdown-menu-right p-3 shadow animated--grow-in"
              aria-labelledby="searchDropdown">
              <form class="form-inline mr-auto w-100 navbar-search">
                <div class="input-group">
                  <input type="text" class="form-control bg-light border-0 small" placeholder="Search for..."
                    aria-label="Search" aria-describedby="basic-addon2">
                  <div class="input-group-append">
                    <button class="btn btn-primary" type="button">
                      <i class="fas fa-search fa-sm"></i>
                    </button>
                  </div>
                </div>
              </form>
            </div>
            </li>

            <!-- Nav Item - Alerts -->
            <li class="nav-item dropdown no-arrow mx-1">
              <a class="nav-link dropdown-toggle" href="/app/support" id="alertsDropdown" role="button"
                data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">
                <i class="fas fa-bell fa-fw"></i>
                <!-- Counter - Alerts -->
                <span class="badge badge-danger badge-counter">{{supportCount}}</span>
              </a>
              <!-- Dropdown - Alerts -->
              <div class="dropdown-list dropdown-menu dropdown-menu-right shadow animated--grow-in"
                aria-labelledby="alertsDropdown">
                <h6 class="dropdown-header"><i class="fas fa-life-ring fa-lg fa-fw mr-2 text-danger-400"></i>
                  Support Center
                </h6>
                {{> listAlert supports }}
                <a class="dropdown-item text-center small text-gray-500" href="/app/support">Show All Messages</a>
              </div>
            </li>

            <div class="topbar-divider d-none d-sm-block"></div>

            <!-- Nav Item - User Information -->
            <li class="nav-item dropdown no-arrow">
              <a class="nav-link dropdown-toggle" href="#" id="userDropdown" role="button" data-toggle="dropdown"
                aria-haspopup="true" aria-expanded="false">
                <span class="mr-2 d-none d-lg-inline text-gray-600 small">{{user.name}}</span>
                <img id="avatarLayout" class="img-fluid img-profile rounded-circle float-left" src="{{user.imgPath}}">
              </a>
              <!-- Dropdown - User Information -->
              <div class="dropdown-menu dropdown-menu-right shadow animated--grow-in" aria-labelledby="userDropdown">
                <a class="dropdown-item" href="/app/user">
                  <i class="fas fa-user fa-sm fa-fw mr-2 text-gray-400"></i>
                  Profile
                </a>
                <a class="dropdown-item" href="/app/support">
                  <i class="fas fa-life-ring fa-sm fa-fw mr-2 text-gray-400"></i>
                  Support
                </a>
                <a class="dropdown-item" href="/app/logs">
                  <i class="fas fa-list fa-sm fa-fw mr-2 text-gray-400"></i>
                  Activity Log
                </a>
                <div class="dropdown-divider"></div>
                <a class="dropdown-item" href="#" data-toggle="modal" data-target="#logoutModal">
                  <i class="fas fa-sign-out-alt fa-sm fa-fw mr-2 text-gray-400"></i>
                  Logout
                </a>
              </div>
            </li>
          </ul>
        </nav>
        <!-- End of Topbar -->

        <!-- Begin Page Content -->
        <div class="container col-md-12 col-xl-12">

          {{{body}}}

        </div>

        <!-- /.container-fluid -->
      </div>
      <!-- End of Main Content -->
      <!-- Footer -->
      <footer class="sticky-footer bg-white">
        <div class="container my-auto">
          <div class="copyright text-center text-gray my-auto">
            <span>Copyright &copy; Kiwi Exchange
              <script>
                document.write(new Date().getFullYear());
              </script></span>
          </div>
        </div>
      </footer>
      <!-- End of Footer -->
    </div>
    <!-- End of Content Wrapper -->
  </div>
  <!-- End of Page Wrapper -->

  <!-- Scroll to Top Button-->
  <a class="scroll-to-top rounded" href="#page-top">
    <i class="fas fa-angle-up"></i>
  </a>

  <!-- Logout Modal-->
  <div class="modal fade" id="logoutModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
    aria-hidden="true">
    <div class="modal-dialog" role="document">
      <div class="modal-content">
        <div class="modal-header">
          <h5 class="modal-title" id="exampleModalLabel">Ready to Leave?</h5>
          <button class="close" type="button" data-dismiss="modal" aria-label="Close">
            <span aria-hidden="true">×</span>
          </button>
        </div>
        <div class="modal-body">Select "Logout" below if you are ready to end your current session.</div>
        <div class="modal-footer">
          <button class="btn btn-secondary float-left" type="button" data-dismiss="modal">Cancel</button>
          <a class="btn btn-primary" href="/auth/logout">Logout</a>
        </div>
      </div>
    </div>
  </div>
</body>

</html>
<script src="/javascripts/app.js"></script>